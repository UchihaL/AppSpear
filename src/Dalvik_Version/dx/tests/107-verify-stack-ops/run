function oneop()
{
    jasmin -d . op_"$1".j
    dx --debug --dex op_"$1".class >/dev/null 2>&1
    if [ "$?" = "0" ]; then
        dx --debug --dex --dump-method="op_$1.test*" op_"$1".class
    else
        echo "$1: expected failure occurred"
    fi
}

oneop dup
oneop dup_x1_case1
oneop dup_x1_case2
oneop dup_x2_case1
oneop dup_x2_case2
oneop dup_x2_case3
oneop dup2
oneop dup2_x1_case1
oneop dup2_x1_case2
oneop dup2_x1_case3
oneop dup2_x2_case1
oneop dup2_x2_case2
oneop dup2_x2_case3
oneop pop
oneop pop2
oneop swap_case1
oneop swap_case2
