function oneop()
{
    jasmin -d . op_"$1".j
    dx --debug --dex op_"$1".class >/dev/null 2>&1
    if [ "$?" = "0" ]; then
        dx --debug --dex --dump-method="op_$1.test*" op_"$1".class
    else
        echo "$1: expected failure occurred"
    fi
}

oneop f2d
oneop f2i
oneop f2l
oneop fadd
oneop fdiv
oneop fmul
oneop fneg
oneop frem
oneop fsub
oneop i2d
oneop i2f
oneop i2l
oneop iadd
oneop iand
oneop idiv
oneop imul
oneop ineg
oneop ior
oneop irem
oneop ishl
oneop ishr
oneop isub
oneop iushr
oneop ixor
